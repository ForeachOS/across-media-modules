<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- utility objects -->
		<typeAlias alias="Fraction" type="com.foreach.imageserver.business.Fraction"/>

		<!-- business objects -->
		<typeAlias alias="Application" type="com.foreach.imageserver.business.Application"/>
		<typeAlias alias="Image" type="com.foreach.imageserver.business.Image"/>
		<typeAlias alias="Dimensions" type="com.foreach.imageserver.business.Dimensions"/>
	</typeAliases>

	<mappers>
		<mapper resource="com/foreach/imageserver/dao/ApplicationDao.xml"/>
		<mapper resource="com/foreach/imageserver/dao/ImageDao.xml"/>
	</mappers>
</configuration>