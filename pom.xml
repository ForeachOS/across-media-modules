<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>com.foreach.across</groupId>
		<artifactId>across-standard-module-parent</artifactId>
		<version>6.0.0-SNAPSHOT</version>
	</parent>

	<groupId>com.foreach.across.modules</groupId>
	<artifactId>web-cms-module-project</artifactId>
	<packaging>pom</packaging>
	<version>${revision}</version>

	<description>
		Provides infrastructure and administration UI for a basic Web CMS.
	</description>
	<url>https://across.dev/modules/WebCmsModule</url>
	<licenses>
		<license>
			<name>The Apache License, Version 2.0</name>
			<url>https://www.apache.org/licenses/LICENSE-2.0.txt</url>
		</license>
	</licenses>

	<developers>
		<developer>
			<name>Foreach</name>
			<organization>Foreach</organization>
			<organizationUrl>https://www.foreach.be</organizationUrl>
		</developer>
	</developers>

	<repositories>
		<repository>
			<id>across</id>
			<name>Across Repository</name>
			<url>https://repository.foreach.be/nexus/repository/public/</url>
		</repository>
	</repositories>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<revision>0.2.0-SNAPSHOT</revision>
		<webjars.bootbox>5.3.2</webjars.bootbox>
		<webjars.codemirror>5.44.0</webjars.codemirror>
		<webjars.lodash>4.17.4</webjars.lodash>
		<webjars.jquery.ui>1.12.1</webjars.jquery.ui>
	</properties>

	<modules>
		<module>web-cms-module</module>
		<module>web-cms-module-test</module>
	</modules>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>com.foreach.across.modules</groupId>
				<artifactId>admin-web-module</artifactId>
				<version>5.0.0-SNAPSHOT</version>
			</dependency>
			<dependency>
				<groupId>com.foreach.across.modules</groupId>
				<artifactId>entity-module</artifactId>
				<version>5.0.0-SNAPSHOT</version>
			</dependency>
			<dependency>
				<groupId>com.foreach.across.modules</groupId>
				<artifactId>bootstrap-ui-module</artifactId>
				<version>4.0.0-SNAPSHOT</version>
			</dependency>
			<dependency>
				<groupId>com.foreach.across.modules</groupId>
				<artifactId>across-hibernate-module</artifactId>
				<version>5.0.0-SNAPSHOT</version>
			</dependency>
			<dependency>
				<groupId>com.foreach.across.modules</groupId>
				<artifactId>user-module</artifactId>
				<version>5.0.0-SNAPSHOT</version>
			</dependency>
			<dependency>
				<groupId>com.foreach.across.modules</groupId>
				<artifactId>debug-web-module</artifactId>
				<version>4.0.0-SNAPSHOT</version>
			</dependency>
			<dependency>
				<groupId>com.foreach.imageserver</groupId>
				<artifactId>imageserver-core</artifactId>
				<version>6.0.0.RC1</version>
			</dependency>
			<dependency>
				<groupId>com.foreach.imageserver</groupId>
				<artifactId>imageserver-admin</artifactId>
				<version>6.0.0.RC1</version>
			</dependency>
			<dependency>
				<groupId>com.foreach.imageserver</groupId>
				<artifactId>imageserver-client</artifactId>
				<version>6.0.0.RC1</version>
			</dependency>
			<dependency>
				<groupId>com.foreach.across.modules</groupId>
				<artifactId>ehcache-module</artifactId>
				<version>3.0.0-SNAPSHOT</version>
			</dependency>
			<dependency>
				<groupId>com.cloudinary</groupId>
				<artifactId>cloudinary-http44</artifactId>
				<version>1.26.0</version>
			</dependency>
			<!-- Webjars-->
			<dependency>
				<groupId>org.webjars.npm</groupId>
				<artifactId>codemirror-minified</artifactId>
				<version>${webjars.codemirror}</version>
			</dependency>
			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>lodash</artifactId>
				<version>${webjars.lodash}</version>
			</dependency>
			<dependency>
				<groupId>org.webjars.bower</groupId>
				<artifactId>bootbox.js</artifactId>
				<version>${webjars.bootbox}</version>
			</dependency>
			<dependency>
				<groupId>org.webjars</groupId>
				<artifactId>jquery-ui</artifactId>
				<version>${webjars.jquery.ui}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>

	<dependencies>
		<dependency>
			<groupId>org.junit.jupiter</groupId>
			<artifactId>junit-jupiter-engine</artifactId>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.junit.vintage</groupId>
			<artifactId>junit-vintage-engine</artifactId>
			<scope>test</scope>
		</dependency>
	</dependencies>

	<scm>
		<url>https://bitbucket.org/beforeach/web-cms-module</url>
	</scm>

</project>