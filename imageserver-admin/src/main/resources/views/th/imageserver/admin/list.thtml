<select ng-model="context" ng-change="change()">
       <option value="ONLINE">online</option>
       <option value="DIGITAL">digital</option>
   </select>

   <table>
       <tr ng-repeat="resolution in resolutions | orderBy:['width','height']">
           <td><a href="#/detail/{{resolution.width}}">{{resolution.width}}x{{resolution.height}}</a></td>
       </tr>
   </table>