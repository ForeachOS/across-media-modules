<configuration>
	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d %p [%t] %c - %m%n</pattern>
		</encoder>
	</appender>
	<logger name="com.foreach.imageserver" level="DEBUG">
		<appender-ref ref="CONSOLE"/>
	</logger>
	<root level="DEBUG"/>
</configuration>